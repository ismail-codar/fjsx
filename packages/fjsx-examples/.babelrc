{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false
      }
    ],
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-proposal-export-default-from",
    [
      "@fjsx/transform-fjsx-syntax",
      {
        "exclude": ["*.js", "*.jsx", "*.react.*"]
      }
    ]
  ],
  "retainLines": true
}
